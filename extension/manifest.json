{
  "manifest_version": 2,
  "name": "Lablr",
  "version": "1.0",
  "description": "A Chrome extension for gathering labelable elements",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "permissions": [
    "http://localhost:3000/*"
  ]
}
